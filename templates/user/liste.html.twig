{% extends 'base.html.twig' %}

{% block title %}Utilisateurs{% endblock %}

{% block body %}


    <section class="section">
    <div class="row">
    <div class="col-lg-12">
    <h1>Utilisateur</h1>

    <table class="table  table-striped">
    <thead>
    <tr>
    <th>Id</th>
    <th>Cordial</th>
    <th>Nom</th>
    <th>Prenom</th>
    <th>Fonction</th>
    <th>Telephone</th>
    <th>Email</th>
    <th>Centre</th>
    <th>Compte</th>
    <th>MotPasse</th>
    <th>Role</th>
   
    </tr>
    </thead>
    <tbody>
    {% for user in users %}
    <tr>
    <td>{{ user.id }}</td>
    <td>{{ user.cordial }}</td>
    <td>{{ user.nom }}</td>
    <td>{{ user.prenom }}</td>
    <td>{{ user.fonction }}</td>
    <td>{{ user.telephone }}</td>
    <td>{{ user.email }}</td>
    <td>{{ user.centre }}</td>
    <td>{{ user.compte }}</td>
    <td>{{ user.motPasse }}</td>
    <td>{{ user.role }}</td>
    
    </tr>
    {% else %}
    <tr>
    <td colspan="12">no records found</td>
    </tr>
    {% endfor %}
    </tbody>
    </table>

   
    </div>
    </div>
    </section>


{% endblock %}
